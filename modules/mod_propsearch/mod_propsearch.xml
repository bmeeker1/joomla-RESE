	<version>1.5.0</version>
    <description>A property search module.</description>

    <files>
        <filename>mod_propsearch.xml</filename>
        <filename module="mod_propsearch">mod_propsearch.php</filename>
        <filename>index.html</filename>
        <filename>helper.php</filename>
        <filename>tmpl/default.php</filename>
        <filename>tmpl/index.html</filename>
    </files>
    <params>
			<!--optional paramaters-->
			<param name="mycalendar" type="calendar" default="19-08-2010" label="Select Date" description="Add date" format="%d-%m-%Y" />
			<param name="mytimezone" type="timezones" default="-10" label="Select a timezone" description="" />

			<param type="spacer" default="&lt;b&gt;Advanced parameters&lt;/b&gt;" />
			<param name="module_style" type="text" default="Some text" label="Module Suffix" description="" size="10" />
			<param name="myeditor" type="editors" default="none" label="Select an editor" />

			<!-- <param name="userlist" type="usergroup" default="" label="Select a user group" description="" multiple="yes" />-->

    </params>
</install>
